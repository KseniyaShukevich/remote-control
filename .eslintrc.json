{
  "extends": [
      "airbnb-base",
      "airbnb-typescript/base"
  ],
  "plugins": ["@typescript-eslint"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "project": ["./tsconfig.json"]
  },
  "rules": {
      "no-console": "warn",
      "linebreak-style": ["error", "windows"],
      "import/prefer-default-export": "off",
      "no-await-in-loop": "off"
  }
}
